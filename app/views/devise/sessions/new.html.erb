
<div class="formfail">
<center>
<h2>Sign in</h2>

<%= simple_form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <%= f.error_notification %>

	<%= f.input :email %>

	<%= f.input :password %>

	<%= f.input :remember_me, as: :boolean %>

  <div class="form-actions"><%= f.submit "Sign in", class: "btn btn-info" %></div>
<% end %>
<%= link_to "Forgot your password?", new_password_path(resource_name) %>
<table border="0" bordercolor="" style="background-color:" width="500px" cellpadding="3" cellspacing="3">
	<tr>
		<td><%= link_to image_tag("https://s3.amazonaws.com/failboard/fbsignup.png"), user_omniauth_authorize_path(:facebook)%></td>
		<td><%= link_to 'Sign Up With Your Email', new_user_registration_path, :class => 'btn btn-info pull-right' %></td>
	</tr>
</table>



</center>
</div>